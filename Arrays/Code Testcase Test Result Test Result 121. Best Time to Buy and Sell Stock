T.c -> O(n) , S.c -> o(1)

class Solution {
public:
    int maxProfit(vector<int>& nums) {
        int profit = 0 , buyPrice = nums[0];
        for(int i = 1; i < nums.size(); i++){
            profit = max(profit,nums[i]-buyPrice);
            if(nums[i]-buyPrice < 0) buyPrice = nums[i];
        }

        return profit;
    }
};
